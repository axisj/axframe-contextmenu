{
  "name": "react-frame-contextmenu",
  "version": "0.1.1",
  "jsnext:main": "dist/es6/index.js",
  "module": "dist/es6/index.js",
  "main": "dist/commonjs/index.js",
  "scripts": {
    "dev": "next dev",
    "next:build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:common": "lessc react-frame-contextmenu/style.less dist/style.css && rimraf ./dist/scss && ncp react-frame-contextmenu/style.less ./dist/style.less && ncp react-frame-contextmenu/less ./dist/less  && ncp react-frame-contextmenu/style.scss ./dist/style.scss && ncp react-frame-contextmenu/scss ./dist/scss",
    "build:es5": "rimraf ./dist/commonjs && tsc --p tsconfigs/tsconfig.es5.json",
    "build:es6": "rimraf ./dist/es6 && tsc --p tsconfigs/tsconfig.es6.json",
    "build": "npm run build:common && npm run build:es5 && npm run build:es6",
    "prepublishOnly": "npm run build"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@emotion/styled": "^11.10.0",
    "@types/node": "18.7.13",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "antd": "^4.22.8",
    "eslint": "^8.22.0",
    "eslint-config-next": "12.2.5",
    "less": "^4.1.3",
    "ncp": "^2.0.0",
    "next": "^12.2.5",
    "next-with-less": "^2.0.5",
    "prettier": "^2.7.1",
    "typescript": "4.8.2"
  }
}
